.full_width {
	padding-left:0px;
	padding-right:0px;
}
#mapCanvas {
	border: 1px solid #C0C0C0;
	margin-top: auto;
	margin-right: auto;
	margin-left: auto;
	margin-bottom: auto;
	width: 100%;
	height: 675px;
	float: left;
}
.pro_box {
	max-width:400px;
}
.summary {
	height: 250px;
	border: 1px solid #e8e8e8;
	background-color: #f9f9f9;
	border-radius: 10px;
	-moz-border-radius-topleft: 10px;
	-moz-border-radius-topright: 10px;
	-moz-border-radius-bottomright: 10px;
	-moz-border-radius-bottomleft: 10px;
	padding-left: 15px;
	font-family: Arial, Helvetica, sans-serif;
	color: #660000;
	margin-bottom:20px;
}
.summary ul {
	margin: 0px;
	margin-bottom:10px;
	padding:0px;
}
#contentList {
	margin-top:20px;
}
.main-content {
	border:1px solid #D8D8D8;
	min-height:50px;
	margin-bottom:5px;
	border-radius:5px;
}
.pro-details span {
	vertical-align:middle;
	line-height:50px;
}
.details {
	float:left;
	margin-left:10px;
	margin-right:10px;
}
.options {
	float:right;
}
.option-button {
	margin:7px 5px 0px 0px;
}
.pro-dropdown {
	min-height:100px;
	border-radius:5px;
	border:1px solid #D8D8D8;
	margin-bottom:5px;
}